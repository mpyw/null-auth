{
    "name": "mpyw/null-auth",
    "description": "Null Guard for Laravel. Designed for Middleware-based authentication and testing.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "keywords": [
        "laravel",
        "illuminate",
        "auth",
        "nothing",
        "null",
        "none",
        "dummy"
    ],
    "autoload": {
        "psr-4": {
            "Mpyw\\NullAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpyw\\NullAuth\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/auth": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/contracts": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "orchestra/testbench-core": ">=7.0",
        "phpunit/phpunit": ">=9.5",
        "mockery/mockery": "^1.3.3 || ^1.4.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Mpyw\\NullAuth\\NullAuthServiceProvider"
            ]
        }
    }
}
